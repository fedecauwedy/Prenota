@model dataBase
@{
    ViewData["Title"] = "Prenotato";
}
<h2>Grazie @Model.Utenti.First().Nome</h2>
<p>per esserti registrato! </p>
<p>Sono stati registrati @Model.Utenti.Count() utenti... </p>

<table class="table">
    <tr>
    <thead class= "thead-dark">
        <th>Nome</th>
        <th>Cognome</th>
        <th>Città</th>
        <th>Email</th>
    </tr>
    </thead>
    @{foreach(var item in @Model.Utenti){
    <tr>
        <td>@item.Nome</td>
        <td>@item.Cognome</td>
        <td>@item.Città</td>
        <td>@item.Email</td>
    </tr>
    }
    }
</table>